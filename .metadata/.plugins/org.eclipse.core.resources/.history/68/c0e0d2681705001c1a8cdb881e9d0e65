//Test 302

program test302

const int jedan = 1;

int i, j;
bool ibool;

int niz[];

{
	
	int sum(int z, int d)
	{
		return z + d;
	}
	
	void fibonaci() 
		int pom,l, a[]; 
	{
		pom=len(a);
		
		if (pom<1) return;
		l=2;
		a[0]=0;
		if (pom<2) return;
		a[1]=1;
		
		if(pom<3) return;
		
		do
		{
			a[l]=sum(a[l-1],a[l-2]);
			l++;
		}
		while( l < pom );
		
	}
	
	void main()
	{
				
		niz = new int[7];
		fibonaci();
		i=0;
		
		do
		{
			print(niz[i]);
			i++;
		}
		while( i < len(niz));
		
		print(eol);
		print(i);
	}
}

