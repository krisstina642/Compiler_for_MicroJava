program Glavni 
	float prom; // greska, float tip ne postoji
	char niz[]; // deklaracija niza
{

	int saberi (char a, char b) // parametre ne obradjujemo
	{
		return 5; // greska – return bez izraza
	}

   
   void nista() { return; } // greska – ne sme izraz u return
 
   void main () 
		int r;
	{
		r = saberi(niz[0], niz[1]);
		print(prom);
	// u redu je sto nema return
   }

}
